  "scripts": {
    "clean": "rm -rf dist .ng_build",
    "demo-app": "ng serve",
    "build": "node tools/build.js",
    "build:demo-app": "ng build --prod --base-href=\"/popover/\"",
    "gh-pages": "ngh --dir dist/app",
