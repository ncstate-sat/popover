    "test": "concurrently -r \"node tools/watch-test.js\" \"karma start karma.conf.js\"",
    "watch": "node tools/watch.js",
    "test:once": "node tools/build-test.js && karma start karma.conf.js --single-run"
  },
  "private": true,
  "keywords": [
    "angular",
    "component",
    "popover",
    "popup",
    "overlay"
